{
    "name": "my/demo-project",
    "description": "A demo PHP project for generating a dependency tree",
    "type": "project",
    "license": "MIT",
    "require": {
             "php": "^7.3",
    "guzzlehttp/guzzle": "6.5.5",
    "league/flysystem": "1.1.3",
    "lcobucci/jwt": "3.3.0",
    "symfony/http-kernel": "4.4.0",
    "symfony/security-core": "4.4.0",
    "twig/twig": "1.44.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "DemoProject\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
